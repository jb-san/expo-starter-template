{
  "name": "expo-starter-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "update-project": "node ./scripts/update-project.js && expo-doctor",
    "clean:ios": "rm -rf ios/",
    "clean:node_modules": "rm -rf node_modules/",
    "clean:android": "rm -rf android/",
    "clean:cache": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf .expo/ && rm -rf .expo-shared/",
    "clean:all": "npm run clean:node_modules && npm run clean:ios && npm run clean:android && npm run clean:cache && npm run clean:watchman",
    "clean:watchman": "watchman watch-del-all",
    "nuke": "rm -rf package-lock.json && npm run clean:all"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "expo": "^54.0.29",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.19",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "tailwindcss": "^3.4.16"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "expo-doctor": "^1.12.4",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.16",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
